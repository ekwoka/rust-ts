{
  "name": "@ekwoka/rust-ts",
  "version": "0.4.1",
  "description": "Simple TS implementations of some Rust structures",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./upgrade": {
      "import": "./dist/upgrade/index.js",
      "types": "./dist/upgrade/index.d.ts"
    }
  },
  "files": ["dist", "src"],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "coverage": "vitest run --coverage",
    "doctor": "run-s lint test:run build size",
    "lint": "run-s lint:*",
    "lint:biome": "biome check --write",
    "lint:staged": "lint-staged",
    "lint:check": "biome check",
    "prebuild": "rm -rf dist",
    "prepare": "husky",
    "size": "node scripts/esbuild.js",
    "test": "vitest",
    "test:run": "vitest run",
    "sandbox": "vite ./sandbox --open --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "22.7.4",
    "@vitest/coverage-v8": "2.1.2",
    "esbuild": "0.24.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "npm-run-all2": "6.2.3",
    "pretty-bytes": "6.1.1",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,mjs,json,js}": "biome check --write"
  },
  "pnpm": {
    "overrides": {
      "typescript": "$typescript",
      "esbuild": "$esbuild"
    }
  }
}
