{
  "name": "@ekwoka/rust-ts",
  "version": "0.4.0",
  "description": "Simple TS implementations of some Rust structures",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./upgrade": {
      "import": "./dist/upgrade/index.js",
      "types": "./dist/upgrade/index.d.ts"
    }
  },
  "files": ["dist", "src"],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "coverage": "vitest run --coverage",
    "doctor": "run-s lint test:run build size",
    "lint": "run-s lint:*",
    "lint:biome": "biome check --write",
    "lint:staged": "lint-staged",
    "prebuild": "rm -rf dist",
    "prepare": "husky",
    "size": "node scripts/esbuild.js",
    "test": "vitest",
    "test:run": "vitest run",
    "sandbox": "vite ./sandbox --open --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@types/node": "20.14.2",
    "@vitest/coverage-c8": "0.32.2",
    "esbuild": "0.21.5",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "npm-run-all2": "6.2.0",
    "pretty-bytes": "6.1.1",
    "typescript": "5.4.5",
    "vite": "5.3.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "lint-staged": {
    "!(/)**/*.{ts,tsx,mjs}": "biome check --write",
    "*.json": "biome check --write"
  },
  "pnpm": {
    "overrides": {
      "typescript": "$typescript",
      "esbuild": "$esbuild"
    }
  }
}
